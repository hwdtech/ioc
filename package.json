  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hwdtech/ioc/issues"
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/hwdtech/ioc#readme"
