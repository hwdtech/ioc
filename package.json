{
  "name": "ioc-type",
  "version": "1.0.8",
  "description": "Inversion of control container for Typescript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "tslint --fix --project .",
    "start": "npm run build:live",
    "build": "tsc",
    "build:live": "nodemon --watch './**/*.ts' --exec 'ts-node' ./index.ts",
